{
  "vuejsCode": "<template>\n  <div class=\"container py-5\">\n    <div class=\"card\" style=\"width: 18rem;\">\n      <img class=\"card-img-top\" :src=\"imageUrl\" :key=\"imageUrl\" alt=\"Card image cap\">\n      <div class=\"card-body\">\n        <h5 class=\"card-title\">{{ title  }}</h5>\n        <p class=\"card-text\">{{ text }}</p>\n        <a href=\"#\" class=\"btn btn-primary\">Button</a>\n      </div>\n    </div>\n  </div>\n</template>\n\n\n<script>\nexport default {\n  data() {\n    return {\n      title: '',\n      imageUrl: '',\n      text: '',\n    };\n  },\n    mounted() {\n    fetch('http://localhost:5000/api/data')\n      .then((response) => response.json())\n      .then((data) => {\n        this.title = data.title;\n        this.imageUrl = data.imageUrl;\n        this.text = data.text;\n      })\n      .catch((error) => {\n        console.error('Error fetching data:', error);\n      });\n  }\n};\n</script>\n",
  "cssCode": "@import url('https://fonts.googleapis.com/css2?family=Open+Sans&display=swap');\n\nbody {\n  font-family: 'Open Sans', sans-serif;\n}\n\nh1 {\n  color: blue; \n}\n\n.card {\n  border-radius: 20px;\n  box-shadow: 0 4px 8px rgba(0,0,0,0.1);\n  overflow: hidden;\n}\n\n.card img {\n  object-fit: cover;\n  height: 200px;\n}\n\n\n\n\n\n"
